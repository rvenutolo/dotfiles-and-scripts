#!/usr/bin/env bash

set -euo pipefail

if [[ "$#" -ne 0 ]]; then
  readonly script_name="${0##*/}"
  echo "${script_name}: Expected no arguments" >&2
  exit 2
fi

if [[ -t 0 ]]; then
  readonly script_name="${0##*/}"
  echo "${script_name}: Expected STDIN" >&2
  exit 2
fi

## handy for counting duplicated lines in a file or listing
sort | uniq -c | sort -rn
